<configuration>

    <appender name="STDOUT"
              class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
                %-5level - %msg%n
            </pattern>
        </encoder>
    </appender>


    <logger name="fr.janalyse.ssh" level="DEBUG"/>


    <root level="INFO">
        <appender-ref ref="STDOUT" />
        <appender-ref ref="FILE" />
    </root>

</configuration>